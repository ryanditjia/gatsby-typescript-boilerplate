{
	"name": "example.com",
	"version": "0.1.0",
	"description": "A simple starter to get up and developing quickly with Gatsby and TypeScript",
	"private": true,
	"author": "Ryandi Tjia <ryandi.tjia@me.com>",
	"homepage": "https://example.com/",
	"license": "MIT",
	"keywords": [
		"gatsby",
		"typescript",
		"emotion"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ryanditjia/example.com.git"
	},
	"scripts": {
		"start": "npm run dev",
		"dev": "gatsby develop --host 0.0.0.0",
		"build": "gatsby build",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"gen": "apollo codegen:generate src/types/graphql.ts --endpoint=http://0.0.0.0:8000/___graphql --outputFlat --target=typescript --tagName=graphql --includes='src/**/*.{ts,tsx}' --no-addTypename --watch",
		"format": "prettier --write \"**/*.{js,ts,tsx}\"",
		"lint": "tsc --noEmit && eslint . --ext .js --ext .ts --ext .tsx"
	},
	"dependencies": {
		"@emotion/core": "^10.0.22",
		"dotenv": "^8.2.0",
		"gatsby": "^2.18.6",
		"gatsby-image": "^2.2.34",
		"gatsby-plugin-canonical-urls": "^2.1.16",
		"gatsby-plugin-emotion": "^4.1.16",
		"gatsby-plugin-layout": "^1.1.16",
		"gatsby-plugin-netlify": "^2.1.27",
		"gatsby-plugin-nprogress": "^2.1.15",
		"gatsby-plugin-postcss": "^2.1.16",
		"gatsby-plugin-purgecss": "^4.0.1",
		"gatsby-plugin-react-helmet": "^3.1.16",
		"gatsby-plugin-sitemap": "^2.2.22",
		"gatsby-plugin-typescript": "^2.1.20",
		"gatsby-plugin-webpack-size": "^1.0.0",
		"gatsby-source-filesystem": "^2.1.40",
		"gatsby-transformer-remark": "^5.25.1",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-helmet": "^5.2.1"
	},
	"devDependencies": {
		"@types/react": "^16.9.13",
		"@types/react-dom": "^16.9.4",
		"@types/react-helmet": "^5.0.14",
		"@typescript-eslint/eslint-plugin": "^2.9.0",
		"@typescript-eslint/parser": "^2.9.0",
		"apollo": "^2.21.1",
		"autoprefixer": "^9.7.3",
		"babel-eslint": "^10.0.3",
		"eslint": "^6.7.2",
		"eslint-config-react-app": "^5.0.2",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"postcss-import": "^12.0.1",
		"prettier": "^1.19.1",
		"tailwind-css-variables": "^2.0.3",
		"tailwindcss": "^1.1.4",
		"ts-node": "^8.5.4",
		"typescript": "^3.7.2"
	}
}
